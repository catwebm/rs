local ogID = "136252471123500"
game.Players.LocalPlayer.Character.Humanoid.AnimationPlayed:Connect(function(AnimTrack)
    if AnimTrack.Animation.AnimationId == "rbxassetid://"..ogID then
        AnimTrack:AdjustWeight(-999)
        local newanim = Instance.new("Animation")
        newanim.AnimationId = "rbxassetid://110988881968651"
        local newtrack = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(newanim)
newtrack:Play()

newtrack:AdjustSpeed(1)
AnimTrack.Stopped:Wait()
newtrack:Stop()

    end
end) 

